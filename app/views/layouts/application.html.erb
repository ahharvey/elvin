<!DOCTYPE html>
<html>
<head>
	<title>TestApp | <%= yield (:title) || "Debugging Testbed" %></title>
	<%= csrf_meta_tag %>
	<%= stylesheet_link_tag    "application" %>
	<%= javascript_include_tag "application" %>
	<%= yield :head %>
</head>
<body>
	<div class="container-fluid">
		<div class="row-fluid">
			<%= render 'layouts/navbar' %>
			<%= render 'layouts/alerts' %>
			<div class="page-header">
				<h1><%= yield(:title) %></h1>
			</div>
		    <div class="span10">
				
				
				<%= yield %>
			</div>
		</div>
	</div>

</body>
<div class="no-print">
	<%= debug(params) if Rails.env.development? %>
</div>
</html>
